        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="br.ita.bditac.mobile.alertas.CadastrarEventoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="br.ita.bditac.mobile.alertas.CadastrarEventoActivity" />
        <activity android:name="br.ita.bditac.mobile.alertas.ConsultaEventoActivity" />
        <activity android:name="br.ita.bditac.mobile.alertas.ConsultarIndicadorActivity"></activity>
    </application>

</manifest>
